    </aside>

    <div class="p-4 sm:ml-64">
      <div
        class="border-2 border-gray-200 border-dashed rounded-lg dark:border-gray-700 overflow-x-scroll"
      >
        <div>
          <table
            class="text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400"
          >
            <thead
              class="text-xs text-gray-700 uppercase bg-gray-200 dark:bg-gray-700 dark:text-gray-400"
            >
              <tr>
                <th scope="col" class="px-6 py-3">Requestor</th>
                <th scope="col" class="px-6 py-3">Department Name</th>
                <th scope="col" class="px-6 py-3">SPA ID</th>
                <th scope="col" class="px-6 py-3">Authorization Request</th>
                <th scope="col" class="px-6 py-3">Geographical Area</th>
                <th scope="col" class="px-6 py-3">System Type</th>
                <th scope="col" class="px-6 py-3">Client</th>
                <th scope="col" class="px-6 py-3">Date received</th>
                <th scope="col" class="px-6 py-3">Authorization Status</th>
                <th scope="col" class="px-6 py-3">Ticket Status</th>
              </tr>
            </thead>
            <tbody id="accept" style="display: none">
              {{#each accept}}
              <tr
                class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600"
              >
                <td
                  scope="row"
                  class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white"
                >
                  {{Requestor}}
                </td>
                <td class="px-6 py-4">{{DeptName}}</td>
                <td class="px-6 py-4">{{SAPID}}</td>
                <td class="px-6 py-4">{{Authreq}}</td>
                <td class="px-6 py-4">{{Areaname}}</td>
                <td class="px-6 py-4">{{systype}}</td>
                <td class="px-6 py-4">{{Client}}</td>
                <td class="px-6 py-4">{{Date}}</td>
                <td class="px-6 py-4">{{authstatus}}</td>
                <td class="px-6 py-4">{{ticketstatus}}</td>
              </tr>
              {{/each}}
            </tbody>
            <tbody id="trueforms" style="display: none">
              {{#each trueforms}}
              <tr
                class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600"
              >
                <td
                  scope="row"
                  class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white"
                >
                  {{Requestor}}
                </td>
                <td class="px-6 py-4">{{DeptName}}</td>
                <td class="px-6 py-4">{{SAPID}}</td>
                <td class="px-6 py-4">{{Authreq}}</td>
                <td class="px-6 py-4">{{Areaname}}</td>
                <td class="px-6 py-4">{{systype}}</td>
                <td class="px-6 py-4">{{Client}}</td>
                <td class="px-6 py-4">{{Date}}</td>
                <td class="px-6 py-4">{{authstatus}}</td>
                <td class="px-6 py-4">{{ticketstatus}}</td>

              </tr>
              {{/each}}
            </tbody>
            <tbody id="reject" style="display: none">
              {{#each reject}}
              <tr
                class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600"
              >
                <td
                  scope="row"
                  class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white"
                >
                  {{Requestor}}
                </td>
                <td class="px-6 py-4">{{DeptName}}</td>
                <td class="px-6 py-4">{{SAPID}}</td>
                <td class="px-6 py-4">{{Authreq}}</td>
                <td class="px-6 py-4">{{Areaname}}</td>
                <td class="px-6 py-4">{{systype}}</td>
                <td class="px-6 py-4">{{Client}}</td>
                <td class="px-6 py-4">{{Date}}</td>
                <td class="px-6 py-4">{{authstatus}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>

    {{!-- prettier-ignore --}} 
    <script>
      function show(filter) {
        console.log("Showing filter:", filter); // Debugging line
        document.getElementById("accept").style.display = "none";
        document.getElementById("reject").style.display = "none";
        document.getElementById("trueforms").style.display = "none";

        if (filter === "accept") {
          document.getElementById("accept").style.display = "table-row-group";
        } else if (filter === "reject") {
          document.getElementById("reject").style.display = "table-row-group";
        } else if (filter === "trueforms") {
          document.getElementById("trueforms").style.display =
            "table-row-group";
        }
      }
    </script>
  </body>
</html>
